<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.whz.gdp2.g8.smshandy.SmsHandyInfoController">
   <children>
      <VBox prefHeight="500.0" prefWidth="400.0">
         <children>
            <Label text="SmsHandy Info">
               <font>
                  <Font size="24.0" />
               </font>
               <VBox.margin>
                  <Insets left="120.0" />
               </VBox.margin>
            </Label>
            <Pane prefHeight="353.0" prefWidth="400.0">
               <children>
                  <Label layoutX="35.0" layoutY="48.0" text="Number" />
                  <Label layoutX="35.0" layoutY="78.0" text="Provider" />
                  <Label layoutX="35.0" layoutY="108.0" text="Tarif" />
                  <Label layoutX="34.0" layoutY="138.0" text="Balance" />
                  <Button fx:id="sentButton" layoutX="33.0" layoutY="192.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="118.0" text="Sent Messages" />
                  <Button layoutX="33.0" layoutY="240.0" mnemonicParsing="false" text="Received Messages" />
                  <Label fx:id="numberLabel" layoutX="123.0" layoutY="48.0" text="Label" />
                  <Label fx:id="providerLabel" layoutX="123.0" layoutY="78.0" text="Label" />
                  <Label fx:id="tarifLabel" layoutX="123.0" layoutY="108.0" text="Label" />
                  <Label fx:id="balanceLabel" layoutX="123.0" layoutY="138.0" text="Label" />
               </children>
            </Pane>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <ButtonBar prefHeight="40.0" prefWidth="400.0">
                    <buttons>
                      <Button mnemonicParsing="false" text="New Message" fx:id="newMessageButton">
                           <opaqueInsets>
                              <Insets right="30.0" />
                           </opaqueInsets>
                        </Button>
                        <Button fx:id="changeProviderButton" mnemonicParsing="false" text="Chage Provider" />
                        <Button fx:id="backButton" mnemonicParsing="false" text="Back" />
                    </buttons>
                     <padding>
                        <Insets left="30.0" />
                     </padding>
                     <opaqueInsets>
                        <Insets right="50.0" />
                     </opaqueInsets>
                  </ButtonBar>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
